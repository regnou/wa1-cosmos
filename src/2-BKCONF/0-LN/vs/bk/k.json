[
    // ###
    // ### SNIPPETS - axel (vscode)
    // ###
    {
      "key": "cmd+shift+l",
      "command": "editor.action.insertSnippet",
      "when": "editorTextFocus",
      "args": {
        "snippet": "console.log($1);" // -1
      }
    },
    {
      "key": "cmd+shift+s",
      "command": "editor.action.insertSnippet",
      "when": "editorTextFocus",
      "args": {
        "snippet": "const $1 = ($2) => {\n\t$3\n} ///"
      }
    },
    // ###
    // ###
    // ###
    {
      "key": "cmd+s",
      "command": "workbench.action.files.saveAll"
    },
